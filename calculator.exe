a = int(input("Enter a number: "))
b = int(input("Enter unother number: "))
c = input("Enter an opperator: ")
if  (c == "+"):
    print(a + b)
elif (c == "-"):
    print(a - b)
elif (c == "*"):
    print(a * b)
elif (c == "/"):
    print(a / b)
else :
    print("please Enter a valid opperator")